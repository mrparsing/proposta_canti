{
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          { "key": "Cache-Control", "value": "max-age=0, no-cache, no-store, must-revalidate" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/xxxiii-domenica-ordinario-anno-b",
        "destination": "/celebrazioni/33/B/ordinario"
      },
      {
        "source": "/solennita-di-cristo-re-anno-b",
        "destination": "/celebrazioni/Solennita-Cristo-Re/B/ordinario"
      }
    ]
  }
}